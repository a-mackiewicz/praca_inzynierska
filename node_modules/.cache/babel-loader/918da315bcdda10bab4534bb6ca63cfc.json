CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-6 offset-lg-3 col-sm-10 offset-sm-1\">\r\n        <form class=\"text-center border border-primary p-5\" style=\"margin-top:70px;height:auto;padding-top:100px !important;\" @submit.prevent=\"registerUser\">\r\n          <input type=\"text\" id=\"name\" class=\"form-control mb-5\" placeholder=\"Name\" v-model=\"register.name\" required />\r\n          <input type=\"email\" id=\"email\" class=\"form-control mb-5\" placeholder=\"Email\" v-model=\"register.email\" required />\r\n\r\n          <!-- Password -->\r\n          <input type=\"password\" id=\"password\" class=\"form-control mb-5\" placeholder=\"Password\" v-model=\"register.password\" />\r\n          <p>\r\n            Already have an account??<router-link to=\"/\">click here</router-link>\r\n\r\n            <!-- Sign in button -->\r\n            <center>\r\n              <button class=\"btn btn-primary btn-block w-75 my-4\" type=\"submit\">\r\n                Sign in\r\n              </button>\r\n            </center>\r\n          </p>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport swal from \"sweetalert\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      register: {\r\n        name: \"\",\r\n        email: \"\",\r\n        password: \"\"\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    async registerUser() {\r\n      try {\r\n        let response = await this.$http.post(\"/api/register\", this.register);\r\n        console.log(response);\r\n        let token = response.data.token;\r\n        if (token) {\r\n          localStorage.setItem(\"jwt\", token);\r\n          this.$router.push(\"/\");\r\n          swal(\"Success\", \"Registration Was successful\", \"success\");\r\n        } else {\r\n          swal(\"Error\", \"Something Went Wrong\", \"Error\");\r\n        }\r\n      } catch (err) {\r\n        let error = err.response;\r\n        if (error.status == 409) {\r\n          swal(\"Error\", error.data.message, \"error\");\r\n        } else {\r\n          swal(\"Error\", error.data.err.message, \"error\");\r\n        }\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}